<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.datnguyen.fitness">

    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.ACCESS_WIFI_STATE" />
    <uses-permission android:name="android.permission.ACCESS_NETWORK_STATE" />

    <application
        android:name=".Others.NotificationChannel"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".Activity.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".Activity.ListWeatherActivity" />
        <activity android:name=".Activity.ListExercise" />
        <activity android:name=".Activity.ViewExercise" />
        <activity android:name=".Activity.SettingPage" />

        <receiver android:name=".Others.AlarmNotificationReceiver" />

        <activity android:name=".Activity.CardioTraining" />
        <activity android:name=".Activity.Calendar" />
        <activity android:name=".Activity.ListYoutubeExercise" />
        <activity android:name=".Activity.PlayYoutubeVideo"></activity>
    </application>

</manifest>